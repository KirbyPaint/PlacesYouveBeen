<form action="/places" method="post">
  <label for="cityName">Add a new place</label>
  <input id="cityName" name="cityName" type="text">
  <label for="stayTime">How many days did you stay?</label>
  <input id="stayTime" name="stayTime" type="number">
  <div>
  <input id="fileupload" type="file" multiple="multiple" name="files" />
  </div>
  <button type="submit">Submit</button>
</form>